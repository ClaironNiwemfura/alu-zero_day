    1  cd alu-zero_day
    2  cd git
    3  git pull
    4  touch up_to_date
    5  echo "git pull origin master" > up_to_date
    6  less up_to_date
    7  echo "Update README.md" > up_to_date
    8  git add .
    9  git commit -m "How to be up to date in git"
   10  git push
